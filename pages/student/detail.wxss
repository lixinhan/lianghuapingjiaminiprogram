/**index.wxss**/
page{
  min-height: 100%;  
  /* background-color: #134d9a; */
}
.container{
  height: 100%;
  overflow: hidden;
  display: block;
}
.classlist{ display: block; text-align: center; font-size: 20px;}

.studentlist{
  width: 100%;
  height: 200rpx;
  overflow: hidden;
}
.image{
  float: left;
  overflow: hidden;
  height: 200rpx;
  width: 200rpx;
}
.image image{
  height: 180rpx;
  width: 180rpx;
  margin: 10rpx;
}
.info{
  float: left;
  width: 520rpx;
  overflow: hidden;
}
.info view{
  display: block;
  float: left;
}
.next{
  line-height: 200rpx;
  color:#dbdbdb
}
.student_name{width: 250rpx; line-height:80rpx; height: 80rpx; font-size: 40rpx;} 
.class_name{width: 270rpx;line-height:80rpx; height: 80rpx;}
.student_number{width: 300rpx; height: 60rpx; line-height: 60rpx;}
.major_name{width: 220rpx;height: 60rpx; line-height: 60rpx;}
.total_socre{width:200rpx;height: 60rpx; line-height: 60rpx;}
.student_add_score{width:150rpx;height: 60rpx; line-height: 60rpx;}
.student_reduce_score{width:150rpx;height: 60rpx; line-height: 60rpx;}

.result{font-size: 13px;color: #323232;}
